    <bp:STOICHIOMETRIC-COEFFICIENT rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</bp:STOICHIOMETRIC-COEFFICIENT>
    <bp:PHYSICAL-ENTITY rdf:resource="#smallMolecule33261"/>
    <bp:CELLULAR-LOCATION rdf:resource="#openControlledVocabulary33182"/>
