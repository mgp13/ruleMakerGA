                <bp:SYNONYMS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GpmA</bp:SYNONYMS>
                <bp:SEQUENCE rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MAVTKLVLVRHGESQWNKENRFTGWYDVDLSEKGVSEAKAAGKLLKEEGYSFDFAYTSVLKRAIHTLWNVLDELDQAWLPVEKSWKLNERHYGALQGLNKAETAEKYGDEQVKQWRRGFAVTPPELTKDDERYPGHDPRYAKLSEKELPLTESLALTIDRVIPYWNETILPRMKSGERVIIAAHGNSLRALVKYLDNMSEEEILELNIPTGVPLVYEFDENFKPLKRYYLGNADEIAAKAAAVANQGKAK</bp:SEQUENCE>
                <bp:ORGANISM rdf:resource="#bioSource33232"/>
